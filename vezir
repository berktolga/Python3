
esirler=[]
ölenler=[]

n=int(input("Esir sayisini giriniz:"))
v=int(input("Vezirin söyledigi sayi:"))
x=n

for i in range(1,n+1):
    esirler.append(i)

print("esirler",esirler)

while(len(esirler)!=1):
    a=v%x
    c=a-1

    ölenler.append(esirler[c])
    print("{} öldü...".format(esirler[c]))
    esirler.pop(c)

    x-=1

print("ölenler",ölenler)
print("hayatta kalan esir",esirler)
